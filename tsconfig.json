{
   "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "lib": ["ESNext", "DOM"],
      "moduleResolution": "Node",
      "strict": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "strictNullChecks": true,
      "types": ["vite/client"],
      "noEmit": true,
      "noUnusedLocals": false,
      "noUnusedParameters": true,
      "noImplicitReturns": true
   },
   "include": ["./src"]
}
